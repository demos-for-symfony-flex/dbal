{
    "name": "symfony-flex-demo/dbal",
    "type": "project",
    "license": "MIT",
    "description": "A minimal DBAL Flex project",
    "require": {
        "doctrine/doctrine-bundle": "^1",
        "symfony/flex": "^1.0",
        "symfony/maker-bundle": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "initialize": [
            ""
        ],
        "security:check": [
            "security-checker security:check"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
